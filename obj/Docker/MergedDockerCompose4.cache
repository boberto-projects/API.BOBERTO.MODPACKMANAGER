services:
  minecraftserver.api:
    build:
      context: C:\Users\gedeo\source\repos\MinecraftServer.Api
      dockerfile: MinecraftServer.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MinecraftServer.Api
      target: base
    container_name: MinecraftServer.Api_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: minecraftserverapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/MinecraftServer.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 80
    - target: 80
    tty: true
    volumes:
    - C:\Users\gedeo\source\repos\MinecraftServer.Api\MinecraftServer.Api:/app:rw
    - C:\Users\gedeo\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\gedeo\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\gedeo\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\gedeo\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\gedeo\source\repos\MinecraftServer.Api:/src:rw
  redis_compose:
    hostname: redis
    image: redis
    ports:
    - published: 6379
      target: 6379
    restart: always
version: '3.4'