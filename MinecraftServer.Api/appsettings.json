{
  "ConnectionStrings": {
    "Redis": "%REDIS_URL%"
  },
  "ApiConfig": {
    "Swagger": true,
    "ModPackDirUrl": "https://modpackmanager.robertocpaes.dev/files",
    "VersionDirUrl": "https://modpackmanager.robertocpaes.dev/launcher",
    "CaminhoLauncherVersion": "webfiles/launcherversion",
    "CaminhoModPacks": "webfiles/modpacks",
    "Authorization": {
      "Activate": true,
      "ApiHeader": "ApiKey",
      "ApiKey": "%API_KEY%"
    }
  },
  "MongoConnections": {
    "CollectionSettings": [
      {
        "ConnectionString": "%MONGO_URL%",
        "DatabaseName": "mongo_service",
        "CollectionName": "ModPacks"
      },
      {
        "ConnectionString": "%MONGO_URL%",
        "DatabaseName": "mongo_service",
        "CollectionName": "LauncherVersion"
      },
      {
        "ConnectionString": "%MONGO_URL%",
        "DatabaseName": "mongo_service",
        "CollectionName": "Config"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
